
<div class="tracking-ffs-container">
    <div class="card mb-10">
        <div class="card-header border-0 pt-6">
            <div class="card-toolbar" style="width: 100%;">
                <div class="row" style="width: 100%;">
                    <div class="col-md-4">
                        <div class="card-title">
                            <div class="fs-1"><span>FFS Tracking |</span>&nbsp;&nbsp;<span class="fs-1 text-primary" id="clinic-year"></span></div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <select id="calendar-clinics" aria-label="clinics" class="form-select form-select-lg form-select-solid" data-hide-search="false" data-control="select2" data-placeholder="All Clinics" data-allow-clear="true" multiple="multiple">
                        </select>
                    </div>
                    <div class="col-md-2">
                        <div class="d-flex justify-content-end mx-10">
                            <select id="calendar-year" aria-label="Year" data-control="select2" data-hide-search="false" class="form-select mb-2 mx-5">
                            </select>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="card-body py-4">
            <div class="calendar table-responsive">
                <table id="tracking_calendar_table" class="table table-row-dashed tatle-row-gray-400 align-middle text-upcase">
                    <thead>
                        <tr class="fw-bold fs-6 text-gray-800">
                            <th>Clinic Name</th>
                            <th class="text-center">Jan</th>
                            <th class="text-center">Feb</th>
                            <th class="text-center">Mar</th>
                            <th class="text-center">Apr</th>
                            <th class="text-center">May</th>
                            <th class="text-center">Jun</th>
                            <th class="text-center">Jul</th>
                            <th class="text-center">Aug</th>
                            <th class="text-center">Sup</th>
                            <th class="text-center">Oct</th>
                            <th class="text-center">Nov</th>
                            <th class="text-center">Dec</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
    <div class="card" id="card-ffs" data-kt-scroll-offset="0">
        <div class="card-header border-0 pt-6">
            <div class="card-title">
                <div class="fs-1"><span class="text-primary" id="clinic-name"></span></div>
            </div>
            <div class="card-toolbar" style="width: 100%;">
                <div class="row" style="width: 100%;">
                    <div class="col-md-10 search-component">
                        <div class="row mx-1">
                            <div class="col-md-4">
                                <div class="d-flex align-items-center position-relative">
                                    <i class="ki-duotone ki-magnifier fs-3 position-absolute ms-5">
                                        <span class="path1"></span>
                                        <span class="path2"></span>
                                    </i>
                                    <input id="search_all" type="text" data-kt-user-table-filter="search" class="form-control form-control-solid ps-13" placeholder="Search ..."/>
                                </div>
                            </div>
                            <div class="col-md-2 d-flex align-items-center position-relative">
                                <div class="form-check form-check-custom form-check-primary form-check-solid mx-auto"></div>
                            </div>
                            <div class="col-md-3">
                                <div class="header-search d-flex align-items-center show menu-dropdown" data-kt-menu-trigger="auto" data-kt-menu-permanent="true" data-kt-menu-placement="bottom-start">
                                    <select id="search_type" aria-label="Select a Visit Type" data-control="select2" data-hide-search="true" data-placeholder="Select a Visit Type..." class="form-select mb-2">
                                    </select>
                                </div>
                            </div>
                            <!-- <div class="col-md-3">
                                <div class="header-search d-flex align-items-center show menu-dropdown" data-kt-menu-trigger="auto" data-kt-menu-permanent="true" data-kt-menu-placement="bottom-start">
                                    <select id="search_status" aria-label="Select a Visit Status" data-control="select2" data-hide-search="true" data-placeholder="Select a Visit Status..." class="form-select mb-2">
                                    </select>
                                </div>
                            </div> -->
                        </div>
                    </div>
                    <div class="col-md-2 export-component component-end">
                        <!--begin::Export dropdown-->
                        <button type="button" class="btn btn-light-primary" id="ffs-export">
                            <i class="ki-duotone ki-exit-down fs-2"><span class="path1"></span><span class="path2"></span></i>
                            Export
                        </button>
                    </div>
                </div>
            </div>
        </div>
        <div class="card-body py-4">
            <table class="table table-striped table-row-bordered border table-bordered rounded align-middle text-upcase gy-4 gs-6" id="tracking_ffs_table" >
                <thead>
                    <tr>
                        <th class="px-3 text-center" style="width: 7%;">Date</th>
                        <th class="px-3 text-center" style="width: 8%;">Insurance</th>
                        <th class="px-3 text-center" style="width: 7%;">LOB</th>
                        <th class="px-3 text-center" style="width: 5%;">EMR ID</th>
                        <th class="px-3 text-center" style="width: 10%;">Source</th>
                        <th class="px-3 text-center" style="width: 17%;">Patient Name</th>
                        <th class="px-3 text-center" style="width: 6%;">DOB</th>
                        <th class="px-3 text-center" style="width: 7%;">Phone</th>
                        <th class="px-3 text-center" style="width: 4%;">Views</th>
                        <th class="px-3 text-center" style="width: 5%;">Contact</th>
                        <th class="px-3 text-center" style="width: 4%;">Log</th>
                        <th class="px-3 text-center" style="width: 3%;">Seen</th>
                        <th class="px-3 text-center" style="width: 6%;">DOS</th>
                        <th class="px-3 text-center" style="width: 8%;">Visit Type</th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>
</div>

<div  class="modal fade"  id="ffs-tracking-modal"  tabindex="-1"  role="dialog"  aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="modal-title">FFS Tracking</h3>
                <div class="btn btn-icon btn-sm btn-active-light-primary ms-2" data-bs-dismiss="modal" aria-label="Close">
                    <i class="ki-duotone ki-cross fs-1"><span class="path1"></span><span class="path2"></span></i>
                </div>
            </div>
            <div class="modal-body">
                <div class="scroll h-500px px-1">
                    <div class="w-600px" id="modal-body-tracking"></div>
                </div>
            </div>
        </div>
    </div>
</div>
