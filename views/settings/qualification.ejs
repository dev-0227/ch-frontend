<div class="card">
    <div class="card-header border-0 pt-6">
      <div class="card-title">
        <h3 class="card-title">Qualifications</h3>
      </div>
      <div class="card-toolbar">
        <a class="btn btn-primary qualificationaddbtn" id="add_btn" ><i class="fa fa-plus">&nbsp;</i>Add Qualification</a>
      </div>
    </div>
    <div class="card-body py-4">
      <input type="hidden" id="chosen_qualification" />
      <table class="table table-striped table-bordered text-nowrap  w-100"  id="qualification_table" >
        <thead>
          <tr >
            <th class="col-md-1">Code</th>
            <th class="col-md-3">System</th>
            <th class="col-md-3">Display</th>
            <th class="col-md-3">Definition</th>
            <th class="text-center col-md-2">Actions</th>
          </tr>
        </thead>
        <tbody></tbody>
      </table>
    </div>
  </div>
<!-- ROW CLOSED -->
<div class="modal fade" id="qualification-edit-modal" tabindex="-1" qualification="dialog"  aria-hidden="true">
	<div class="modal-dialog modal-lg" qualification="document">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title" id="example-Modal3">Edit Qualification</h5>
				<div class="btn btn-icon btn-sm btn-active-light-primary ms-2" data-bs-dismiss="modal" aria-label="Close">
            <i class="ki-duotone ki-cross fs-1"><span class="path1"></span><span class="path2"></span></i>
        </div>
			</div>
			<div class="modal-body">
        <div class="row">
          <div class="col-md-12">
            <div class="form-group">
                <label for="ucode" class="required fw-semibold fs-6 mb-2">Code:</label>
                <input type="text" class="form-control form-control-solid mb-3 mb-lg-0" id="ucode">
            </div>
          </div>
          <div class="col-md-12">
              <div class="form-group">
                  <label for="usystem" class="required fw-semibold fs-6 mb-2">System:</label>
                  <input type="text" class="form-control form-control-solid mb-3 mb-lg-0" id="usystem">
              </div>
          </div>
          <div class="col-md-12">
              <div class="form-group">
                  <label for="udisplay" class="required fw-semibold fs-6 mb-2">Display:</label>
                  <input type="text" class="form-control form-control-solid mb-3 mb-lg-0" id="udisplay">
              </div>
          </div>
          <div class="col-md-12">
            <div class="form-group">
                <label for="udefinition" class="fw-semibold fs-6 mb-2">Definition:</label>
                <input type="text" class="form-control form-control-solid mb-3 mb-lg-0" id="udefinition">
            </div>
        </div>
        </div>
			</div>
			<div class="modal-footer">
        <button type="button" class="btn btn-info" data-bs-dismiss="modal"><i class="fa fa-close">&nbsp;</i> Close</button>
        <button type="button" class="btn btn-primary" id="update_btn"><i class="fa fa-save">&nbsp;</i> Save</button>
      </div>
		</div>
	</div>
</div>
<div class="modal fade" id="qualification-add-modal" tabindex="-1" qualification="dialog"  aria-hidden="true">
	<div class="modal-dialog" qualification="document">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title" id="example-Modal3">Add New Qualification</h5>
				<div class="btn btn-icon btn-sm btn-active-light-primary ms-2" data-bs-dismiss="modal" aria-label="Close">
                    <i class="ki-duotone ki-cross fs-1"><span class="path1"></span><span class="path2"></span></i>
                </div>
			</div>
			<div class="modal-body">
          <div class="row">
            <div class="col-md-12">
              <div class="form-group">
                  <label for="acode" class="required fw-semibold fs-6 mb-2">Code:</label>
                  <input type="text" class="form-control form-control-solid mb-3 mb-lg-0" id="acode">
              </div>
            </div>
            <div class="col-md-12">
                <div class="form-group">
                    <label for="asystem" class="required fw-semibold fs-6 mb-2">System:</label>
                    <input type="text" class="form-control form-control-solid mb-3 mb-lg-0" id="asystem">
                </div>
            </div>
            <div class="col-md-12">
                <div class="form-group">
                    <label for="adisplay" class="required fw-semibold fs-6 mb-2">Display:</label>
                    <input type="text" class="form-control form-control-solid mb-3 mb-lg-0" id="adisplay">
                </div>
            </div>
            <div class="col-md-12">
                <div class="form-group">
                    <label for="adefinition" class="fw-semibold fs-6 mb-2">Definition:</label>
                    <input type="text" class="form-control form-control-solid mb-3 mb-lg-0" id="adefinition">
                </div>
            </div>
          </div>
			</div>
			<div class="modal-footer">
                <button type="button" class="btn btn-info" data-bs-dismiss="modal"><i class="fa fa-close">&nbsp;</i> Close</button>
                <button type="button" class="btn btn-primary" id="create_btn"><i class="fa fa-save">&nbsp;</i> Save</button>
             </div>
		</div>
	</div>
</div>
