<div class="card">
    <div class="card-header border-0 pt-6">
        <div class="card-title">
            <h3 class="card-title">Insurance Types</h3>
        </div>
    </div>
    <div class="card-body py-4">
        <div class="row mb-5">
            <div class="col-md-4">
                <a class="btn btn-primary" id="type-add-btn" ><i class="fa fa-plus">&nbsp;</i>Add Insurance Type</a>
            </div>
            <div class="col-md-4"></div>
            <div class="col-md-3">
                <input type="text" class="form-control form-control-solid" placeholder="Input Search Text" id="insurance-type-search">
            </div>
            <div class="col-md-1"></div>
        </div>
        <input type="hidden" id="chosen_type" />
        <table class="table table-striped table-bordered text-nowrap  w-100"  id="insurance-type-table">
            <thead>
                <tr>
                    <th class="col-md-3">Display</th>
                    <th class="col-md-7">Description</th>
                    <th class="text-center col-md-2">Actions</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
  </div>
<!-- ROW CLOSED -->
<div class="modal fade" id="type-edit-modal" tabindex="-1" organization="dialog"  aria-hidden="true">
	<div class="modal-dialog modal-lg" organization="document">
        <input type="hidden" id="type-edit-type"></input>
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title" id="example-Modal3">Edit Organization Type</h5>
				<div class="btn btn-icon btn-sm btn-active-light-primary ms-2" data-bs-dismiss="modal" aria-label="Close">
                    <i class="ki-duotone ki-cross fs-1"><span class="path1"></span><span class="path2"></span></i>
                </div>
			</div>
			<div class="modal-body">
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label for="insurance-type-display" class="required fw-semibold fs-6 mb-2">Display:</label>
                            <input type="text" class="form-control form-control-solid mb-3 mb-lg-0" id="insurance-type-display">
                        </div>
                    </div>
                    <div class="col-md-12">
                        <div class="form-group">
                            <label for="insurance-type-desc" class="fw-semibold fs-6 mb-2">Description:</label>
                            <input type="text" class="form-control form-control-solid mb-3 mb-lg-0" id="insurance-type-desc">
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-info" data-bs-dismiss="modal"><i class="fa fa-close">&nbsp;</i> Close</button>
                <button type="button" class="btn btn-primary" id="type-save-btn"><i class="fa fa-save">&nbsp;</i> Save</button>
            </div>
		</div>
	</div>
</div>
